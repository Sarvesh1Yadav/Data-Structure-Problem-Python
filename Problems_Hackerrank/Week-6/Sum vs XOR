import math
import os
import random
import re
import sys

def sumXor(n):
    # Write your code here
    count = 0
    if n==0:
        return 1
    while n>0:
        if n%2==0:
            count+=1
        n = n//2
    return 2**count

if __name__ == '__main__':
    fptr = open(os.environ['OUTPUT_PATH'], 'w')

    n = int(input().strip())

    result = sumXor(n)

    fptr.write(str(result) + '\n')

    fptr.close()
