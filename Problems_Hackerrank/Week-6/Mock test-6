def palindromeIndex(s):
       # Write your code here
       n = len(s)
       i = 0
       j = n-1
       l = list(s)
       while i<=j:
         if l[i]==l[j]:
            i+=1
            j-=1
            continue
         if l[i]!=l[j]:
              if l[i+1:j+1]==l[i+1:j+1][::-1]:
                  return i
             elif l[i:j]==l[i:j][::-1]:
                  return j
             else:
                  return -1
       return -1
