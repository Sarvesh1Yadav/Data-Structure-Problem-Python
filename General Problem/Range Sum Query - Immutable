class NumArray:

    def __init__(self, nums: List[int]):
        self.nums = nums

    def sumRange(self, left: int, right: int) -> int:
        sum=0
        l=[]
        n= len(self.nums)
        for i in range(n):
            sum+=self.nums[i]
            l.append(sum)
        if left==0:
            return l[right]
        else:
            return l[right]-l[left-1]

        




