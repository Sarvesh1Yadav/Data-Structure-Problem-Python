class Solution:
    def canPartition(self, nums: List[int]) -> bool:
        def isSubsetSum ( arr, sum):
        # code here 
        
                n = len(arr)
                
                t = [[False]*(sum+1) for _ in range(n+1)]
                for i in range(n+1):
                    t[i][0]=True
                
                for i in range(1,n+1):
                    for j in range(1,sum+1):
                        t[i][j] = t[i-1][j]
                        if arr[i-1]<=j:
                            t[i][j]=t[i-1][j-arr[i-1]] or t[i][j]
                return t[n][sum]
        sm=0  
        for i in range(len(nums)):
            sm+=nums[i]
        if sm%2!=0:
            return False
        else:
            return isSubsetSum(nums,sm//2)
