class Solution:
    def isHappy(self, n: int) -> bool:
        def next(num):
            sm=0
            while num>0:
                r = num%10
                sm+=r**2
                num=num//10
            return sm
        slow = n
        fast = n
        while True:
            slow = next(slow)
            fast =  next(next(fast))
            if fast==1:
                return True
            if fast==slow:
                return False
            
            

        
