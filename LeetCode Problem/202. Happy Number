class Solution:
    def isHappy(self, n: int) -> bool:
        l = []
        l.append(n)
        s1 = n
        while True:
                s2  = 0
                while s1!=0:
                    r= s1%10
                    s2+=r**2
                    s1 = s1//10
                if s2==1:
                    return True
                if s2 in l:
                    return False
                l.append(s2)
                s1 = s2
            
                


        
