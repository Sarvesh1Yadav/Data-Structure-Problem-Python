class Solution:
    def sortArray(self, nums: List[int]) -> List[int]:
        n=len(nums)
        for i in range(n):
            m = nums[i]
            indx = i
            for j in range(i+1,n):
                if nums[j]<m:
                    m = nums[j]
                    indx = j
            nums[i],nums[indx] = nums[indx],nums[i]
        return nums
        
