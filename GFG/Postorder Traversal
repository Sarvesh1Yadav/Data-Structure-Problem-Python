'''
class Node:
    def __init__(self, val):
        self.data = val
        self.left = None
        self.right = None
'''

class Solution:
    def postOrder(self, root):
        # code here
        res = []
        def dfs(node):
            if node is not None:
                dfs(node.left)
                dfs(node.right)
                res.append(node.data)
        dfs(root)
        return res
        
        
