'''
# Node Class:
class Node:
    def __init__(self,val):
        self.data = val
        self.left = None
        self.right = None
'''

class Solution:
    def preOrder(self, root):
    # code here
         res = []
         def dfs(node):
             if node is not None:
                 res.append(node.data)
                 dfs(node.left)
                 dfs(node.right)
         dfs(root)
         return res
